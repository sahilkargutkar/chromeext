{
    "name":"drag ext",
    "description":"my ext",
    "version":"0.1.0",
    "manifest_version":2,
    "icons":{
        "16" :"./img16x16.png",
        "32": "./img32x32.png",
        "48":"./img48x48.png",
        "128":"./img128x128.png"

    },
        "background":{
            "scripts":["./background.js"]
        },
        "browser_action":{
            "default_popup":"popup.html"
        },
        "permissions":[
            "tabs",
            "https://www.google.com/*"

        ]


}